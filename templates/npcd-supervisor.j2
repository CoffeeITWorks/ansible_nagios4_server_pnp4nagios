[program:npcd]
priority=400
redirect_stderr=true
command=/usr/local/pnp4nagios/bin/npcd -f /usr/local/pnp4nagios/etc/npcd.cfg
user=nagios
autostart=true
autorestart=true
stdout_logfile=/var/log/supervisor/npcd_log.log
stderr_logfile=/var/log/supervisor/npcd_error.log
stdout_logfile_maxbytes=4MB
stdout_logfile_backups=10
stdout_capture_maxbytes=1MB
stderr_logfile_maxbytes=4MB
stderr_logfile_backups=10
stderr_capture_maxbytes=1MB

